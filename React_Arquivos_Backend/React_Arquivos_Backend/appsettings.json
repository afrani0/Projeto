{
  "ConnectionStrings": {
    "BloggingDatabase": "Server=[SEU_SERVIDOR_SQL];Database=[SEU_BANCO];Trusted_Connection=True;user id=[SEU_ID];password=[SEU_PASSWORD];"
  },

  "DadosArquivo": {
    "URLArquivo": "[SEU_ARQUIVO_URL]",/*caminho onde as imagens podem ser acessadas via http*/
